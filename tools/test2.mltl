-- Example C2PO input file

-- This is an SMV-inspired syntax, including 
-- comments which are preceded by two dashes

STRUCT
  test: {a,b,c: int;};
 
INPUT
  x,y,z: int;
  
  Order: x,y,z;

DEFINE
  t0 = test(x,y,z);
  t1 = test(y,z,x);
  t2 = test(z,x,y);

  t3 = test(x+y,y+z,x+z);


SPEC
  foreach(t:{t0,t1,t2,t3})(t.a == t.b || x>5);
  